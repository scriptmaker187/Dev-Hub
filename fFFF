    local args = {
        [1] = {
            ["teamIcon"] = "",
            ["action"] = "accepted",
            ["teamName"] = clanName or "Ez scripts"  
        }
    }

    
    pcall(function()
        game:GetService("ReplicatedStorage"):WaitForChild("invitationEvent"):FireServer(unpack(args))
        print("Invitation sent with clan name: " .. clanName)
    end)
end


